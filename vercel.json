{
  "version": 2,
  "builds": [
    { "src": "collobrative.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/collobrative.py" }
  ],
  "env": {
    "APP_MODULE": "collobrative:app"
  }
  "excludeFiles": [
    "**/*.pyc",
    "**/__pycache__/**",
    ".env",
    ".git/**",
    ".venv/**",
    "automations/**",
    "docs/**",
    "logdir/**",
    "tests/**",
    "prisma/migrations/**",
    "Dockerfile",
    "README.md",
    "pytest.ini",
    "package.json"
  ]
}
